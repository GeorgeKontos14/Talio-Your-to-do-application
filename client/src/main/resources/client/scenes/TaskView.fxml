<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<!--Pane prefHeight="55.0" prefWidth="578.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.TaskCell" -->

<Pane fx:id="taskPane" prefHeight="55.0" prefWidth="578.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <CheckBox fx:id="statusBox" layoutX="11.0" layoutY="15.0" mnemonicParsing="false" onAction="#changeStatus" prefHeight="27.0" prefWidth="24.0">
         <font>
            <Font size="18.0" />
         </font>
      </CheckBox>
      <Button fx:id="editButton" layoutX="469.0" layoutY="15.0" mnemonicParsing="false" onAction="#edit" text="Edit" />
      <TextField fx:id="taskTitle" layoutX="51.0" layoutY="15.0" prefHeight="26.0" prefWidth="248.0" text="Task" />
      <Button fx:id="removeButton" layoutX="532.0" layoutY="16.0" mnemonicParsing="false" onAction="#remove" text="X" textFill="#fc0505">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="313.0" layoutY="16.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Button fx:id="confirmButton" layoutX="390.0" layoutY="16.0" mnemonicParsing="false" onAction="#confirm" text="Confirm" />
   </children>
</Pane>
